{#
/**
 * @file cities_entity.html.twig
 * Default theme implementation to present Cities entity data.
 *
 * This template is used when viewing Cities entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_cities_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('cities_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
